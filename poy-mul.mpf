program polyMul;
architecture shared (10);
const n=2;
var a, b : array[0..n] of real;
c: array[0..2*n] of real;
i, j: integer;
L: array[0..2*n] of spinlock

procedure mul(i, j: integer);
begin
	lock(L[i+j]);
	c[i+j] := c[i+j] + a[i] * b[j];
	unlock(L[i+j]);
end;

begin
	for i:=0 to n do
		Readln(a[i]);

	for i:=0 to n do
		Readln(b[i]);

	forall i:=0 to n do
		forall j:=0 to n do
			mul(i, j);

	for i:=0 to 2*n do
		Writeln(c[i]);
	
end.